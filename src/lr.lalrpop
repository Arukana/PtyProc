use std::str::FromStr; // LALRPOP pulls in dependencies just like in Rust.
use util;

grammar; // This marks the file as a grammar for LALRPOP to generate code for.

Num: u16 = <s:r"[0-9]+"> => u16::from_str(s).unwrap();

pub Term: util::Sequence = {
    <s:r"[_a-zA-Z]*"> => util::Sequence::Out(s.to_string()), // Haha
    "<^[[" <x:Num> ";" <y:Num> "Ha" => util::Sequence::Goto(x, y), // <^[[4;2Ha
};
